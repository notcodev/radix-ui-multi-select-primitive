{
  "name": "radix-ui-multiselect-primitive",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.14.0",
  "description": "Multiselect primitive for Radix UI",
  "author": {
    "name": "Erik Codev",
    "url": "https://github.com/notcodev"
  },
  "license": "MIT",
  "homepage": "https://github.com/notcodev/radix-ui-multiselect-primitive",
  "repository": {
    "url": "https://github.com/notcodev/radix-ui-multiselect-primitive.git",
    "type": "git"
  },
  "bugs": "https://github.com/notcodev/radix-ui-multiselect-primitive/issues",
  "keywords": [
    "radix-ui",
    "multiselect",
    "primitive"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc -b",
    "clean": "rimraf dist",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:inspect": "eslint-config-inspector",
    "prepare": "husky",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm build && pnpm publish --access public"
  },
  "peerDependencies": {
    "@types/react": "^18 || ^19",
    "@types/react-dom": "^18 || ^19",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@radix-ui/number": "^1.1.1",
    "@radix-ui/primitive": "^1.1.3",
    "@radix-ui/react-collection": "^1.1.7",
    "@radix-ui/react-compose-refs": "^1.1.2",
    "@radix-ui/react-context": "^1.1.2",
    "@radix-ui/react-direction": "^1.1.1",
    "@radix-ui/react-dismissable-layer": "^1.1.11",
    "@radix-ui/react-focus-guards": "^1.1.3",
    "@radix-ui/react-focus-scope": "^1.1.7",
    "@radix-ui/react-popper": "^1.2.8",
    "@radix-ui/react-portal": "^1.1.9",
    "@radix-ui/react-primitive": "^2.1.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-use-callback-ref": "^1.1.1",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@radix-ui/react-use-layout-effect": "^1.1.1",
    "aria-hidden": "^1.2.6",
    "react": "^19",
    "react-dom": "^19",
    "react-remove-scroll": "^2.7.1"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.2.0",
    "@notcodev/eslint": "^1.4.2",
    "@notcodev/prettier": "^1.2.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "~9.22.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vitepress": "^1.6.4"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,cjs,mjs,cts,mts,md,yml,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
